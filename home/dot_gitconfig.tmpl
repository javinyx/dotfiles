[gpg]
	format = ssh
[gpg "ssh"]
	program = /Applications/1Password.app/Contents/MacOS/op-ssh-sign
[commit]
	gpgsign = true
[user]
	signingkey = {{ onepasswordRead "op://Development/Development SSH/public key" }}
	name = Javin
	email = {{ onepasswordRead "op://Development/Development SSH/email" }}
[bash]
	showInformativeStatus = true
